package snake;

public class Perception {

    private Cell N, W, E, S;
    private Food F;

    public Perception(Cell eastCell, Cell northCell, Cell southCell, Cell westCell, Food food) {
        this.N = northCell;
        this.S = southCell;
        this.E = eastCell;
        this.W = westCell;
        this.F = food;
    }

    public Cell getN() {
        return N;
    }

    public Cell getW() {
        return W;
    }

    public Cell getE() {
        return E;
    }

    public Cell getS() {
        return S;
    }

    public  Food getF(){
        return F ;
    }


}
